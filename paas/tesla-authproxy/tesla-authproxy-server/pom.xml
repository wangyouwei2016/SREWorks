    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.6.2</version>
                <configuration>
                    <excludes>
                        <exclude>com/alibaba/tesla/authproxy/component/sentry/**</exclude>
                        <exclude>com/alibaba/tesla/authproxy/web/BucController.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/web/BucUserController.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/web/SyncController.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/web/UserDepartmentController.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/BucUserService.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/impl/AclAuthServiceManager.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/impl/TeslaAppServiceImpl.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/impl/UserServiceImpl.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/impl/SyncServiceImpl.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/impl/BucUserServiceImpl.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/service/impl/UserDepartmentServiceImpl.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/config/database/**</exclude>
                        <exclude>com/alibaba/tesla/authproxy/model/vo/UserInfoVO.java</exclude>
                        <exclude>com/alibaba/tesla/authproxy/api/**</exclude>
                    </excludes>
                </configuration>
            </plugin>
        </plugins>
